https://leetcode.com/contest/weekly-contest-383/problems/ant-on-the-boundary/
class Solution {
    public int returnToBoundaryCount(int[] nums) {
        int ans=0,sum=0;
        for(int i=0;i<nums.length;i++)
        {
            sum=sum+nums[i];
            if(sum==0)
                ans++;
        }
        return ans;
    }
}
